    "gulp-posthtml": "^3.0.0",
    "gulp-prettify": "^0.5.0",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-stylus": "^2.5.0",
    "gulp-svg-symbols": "^3.2.0",
    "gulp-watch": "^5.0.1",
    "gulp-yaml": "^2.0.1",
    "gulp-zip": "^4.0.0",
    "gulp.spritesmith": "^6.7.0",
    "gulplog": "^1.0.0",
    "happypack": "^5.0.0-beta.1",
    "husky": "^0.14.3",
