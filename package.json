    "object-assign": "^4.1.1",
    "omit": "^1.0.1",
    "popper.js": "^1.14.3",
    "promise": "^8.0.1",
    "raf": "^3.4.0",
    "scroll": "^2.0.1",
    "scroll-doc": "^0.2.1",
    "scrollreveal": "^3.4.0",
    "skrollr": "^0.6.26",
